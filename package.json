{
  "name": "@homer/gatsby-baseline",
  "description": "An opinionated Gatsby v2.x starter project.",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "format-n-lint": "npm run lint:fix && npm run format && npm run lint",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "test:lh": "ava tests/performance/lighthouse.js --verbose",
    "lint:ci": "yarn lint --format junit -o results/eslint/result.xml",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:ci": "cypress run --browser chrome --reporter junit --reporter-options 'mochaFile=results/cypress/result.xml'",
    "cy:record": "cypress run --record --key cd256083-c513-4506-89f7-4b74ca5cc79a",
    "test:e2e:dev": "cross-env CYPRESS_SUPPORT=y start-server-and-test dev http://localhost:8000 cy:open",
    "test:e2e:run": "cross-env CYPRESS_SUPPORT=y start-server-and-test develop http://localhost:8000 cy:run",
    "test:e2e:ci": "cross-env CYPRESS_SUPPORT=y start-server-and-test develop http://localhost:8000 cy:run:ci",
    "deploy:ghpages": "npm run build && node ./scripts/deploy-ghpages.js"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "greenkeeper": {
    "ignore": [
      "ava",
      "cypresss",
      "core-js"
    ]
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "@svgr/webpack": "^4.3.1",
    "gatsby": "^2.13.9",
    "gatsby-image": "^2.2.4",
    "gatsby-node-helpers": "^0.3.0",
    "gatsby-plugin-accessibilityjs": "^1.0.3",
    "gatsby-plugin-catch-links": "^2.1.0",
    "gatsby-plugin-csp": "^1.1.3",
    "gatsby-plugin-extract-schema": "^0.0.5",
    "gatsby-plugin-fastclick": "^1.0.1",
    "gatsby-plugin-manifest": "^2.2.1",
    "gatsby-plugin-netlify": "^2.1.0",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "gatsby-plugin-nprogress": "^2.1.0",
    "gatsby-plugin-offline": "^2.2.1",
    "gatsby-plugin-page-creator": "^2.1.2",
    "gatsby-plugin-polyfill-io": "^1.1.0",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.2",
    "gatsby-plugin-react-helmet": "^3.1.0",
    "gatsby-plugin-recaptcha": "^1.0.5",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sass": "^2.1.0",
    "gatsby-plugin-sharp": "^2.2.3",
    "gatsby-plugin-sitemap": "^2.2.1",
    "gatsby-plugin-svgr": "^2.0.2",
    "gatsby-plugin-transition-link": "^1.12.4",
    "gatsby-source-filesystem": "^2.1.2",
    "gatsby-transformer-sharp": "^2.2.1",
    "gsap": "^2.1.3",
    "lodash": "^4.17.13",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "6.0.0-beta",
    "react-intersection-observer": "^8.23.0",
    "react-pose": "^4.0.8",
    "react-recaptcha": "^2.3.10"
  },
  "devDependencies": {
    "ava": "^2.2.0",
    "babel-eslint": "^10.0.2",
    "chrome-launcher": "^0.11.1",
    "core-js": "^3.1.4",
    "cross-env": "^5.2.0",
    "cypress": "^3.4.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "gatsby-cypress": "^0.2.1",
    "gatsby-plugin-eslint": "^2.0.5",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gh-pages": "^2.0.1",
    "lighthouse": "^5.1.0",
    "prettier": "^1.18.2",
    "start-server-and-test": "^1.9.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/homer-yttc/gatsby-baseline"
  },
  "bugs": {
    "url": "https://github.com/homer-yttc/gatsby-baseline/issues"
  }
}
