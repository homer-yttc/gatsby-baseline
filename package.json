{
  "name": "@homer/gatsby-baseline",
  "description": "An opinionated Gatsby v2.x starter project.",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "dev:ssl": "gatsby develop --https",
    "dev:log": "gatsby develop --verbose",
    "deploy:ghpages": "cross-env GHPAGES=1 gatsby build --prefix-paths && node ./scripts/deploy-ghpages.js",
    "ci:lint": "eslint ./ --ext .js,.jsx --quiet && prettier --check ./**/*.{js,jsx,scss} && stylelint --quiet src/**/*.scss",
    "ci:test": "cross-env CYPRESS_SUPPORT=y start-server-and-test develop http://localhost:8000 cy:run:ci",
    "hooks": "npx lefthook install",
    "hooks:run": "npx lefthook run",
    "lint": "npm run lint:format && npm run lint:fix && npm run lint:style",
    "lint:fix": "eslint ./ --ext .js,.jsx --fix --quiet",
    "lint:format": "prettier --write ./**/*.{js,jsx,scss} --loglevel error",
    "lint:style": "stylelint --fix --quiet src/**/*.scss",
    "test": "cypress open",
    "test:build": "cross-env CYPRESS_SUPPORT=y start-server-and-test dev http://localhost:8000 cy:open",
    "test:record": "cypress run --record --key cd256083-c513-4506-89f7-4b74ca5cc79a",
    "test:perf": "ava tests/performance/lighthouse.js --verbose",
    "start": "gatsby develop",
    "serve": "gatsby serve"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "greenkeeper": {
    "ignore": [
      "ava",
      "cypresss",
      "core-js"
    ]
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "@svgr/webpack": "^4.3.1",
    "cssnano": "^4.1.10",
    "gatsby": "^2.13.9",
    "gatsby-image": "^2.2.4",
    "gatsby-node-helpers": "^0.3.0",
    "gatsby-plugin-accessibilityjs": "^1.0.3",
    "gatsby-plugin-canonical-urls": "^2.1.2",
    "gatsby-plugin-catch-links": "^2.1.0",
    "gatsby-plugin-csp": "^1.1.3",
    "gatsby-plugin-extract-schema": "^0.0.5",
    "gatsby-plugin-fastclick": "^1.0.1",
    "gatsby-plugin-manifest": "^2.2.1",
    "gatsby-plugin-netlify": "^2.1.0",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "gatsby-plugin-nprogress": "^2.1.0",
    "gatsby-plugin-offline": "^2.2.1",
    "gatsby-plugin-page-creator": "^2.1.2",
    "gatsby-plugin-polyfill-io": "^1.1.0",
    "gatsby-plugin-postcss": "^2.1.2",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.2",
    "gatsby-plugin-react-helmet": "^3.1.0",
    "gatsby-plugin-recaptcha": "^1.0.5",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sass": "^2.1.0",
    "gatsby-plugin-sharp": "^2.2.3",
    "gatsby-plugin-sitemap": "^2.2.1",
    "gatsby-plugin-svgr": "^2.0.2",
    "gatsby-plugin-transition-link": "^1.12.4",
    "gatsby-source-filesystem": "^2.1.2",
    "gatsby-transformer-sharp": "^2.2.1",
    "gsap": "^2.1.3",
    "lodash": "^4.17.13",
    "node-sass": "^4.12.0",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^12.0.1",
    "postcss-merge-rules": "^4.0.3",
    "postcss-nesting": "^7.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-pxtorem": "^4.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "6.0.0-beta",
    "react-intersection-observer": "^8.23.0",
    "react-pose": "^4.0.8",
    "react-recaptcha": "^2.3.10"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.6.3",
    "ava": "^2.2.0",
    "babel-eslint": "^10.0.2",
    "chrome-launcher": "^0.11.1",
    "core-js": "^3.1.4",
    "cross-env": "^5.2.0",
    "cypress": "^3.4.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "gatsby-cypress": "^0.2.1",
    "gatsby-plugin-eslint": "^2.0.5",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gh-pages": "^2.0.1",
    "lighthouse": "^5.1.0",
    "prettier": "^1.18.2",
    "start-server-and-test": "^1.9.1",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-order": "^3.0.1",
    "stylelint-scss": "^3.9.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/homer-yttc/gatsby-baseline"
  },
  "bugs": {
    "url": "https://github.com/homer-yttc/gatsby-baseline/issues"
  }
}
