{
  "name": "@homer-yttc/gatsby-baseline",
  "description": "An opinionated Gatsby v2.x starter project.",
  "version": "0.1.0",
  "author": "Homer <homer@yourturntocode.com>",
  "license": "MIT",
  "keywords": [
    "gatsby"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "engines": {
    "node": ">=14.2.0"
  },
  "scripts": {
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "build": "gatsby build",
    "build:incrimental": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "build:serve": "cross-env BUILD=1 NODE_ENV=production BUILD_URL=http://localhost:9000 gatsby build && npm run serve",
    "build:prod": "npm install --only=prod && npm run build",
    "build:dev": "cross-env NODE_ENV=development gatsby build --no-uglify",
    "clean": "gatsby clean",
    "clean:node": "rm -rf node_modules && npm i",
    "dev": "gatsby develop",
    "dev:ssl": "gatsby develop -https",
    "deploy:fb": "cross-env FBBUILD=1 FBBUILD_URL=https://gatsby-baseline.firebaseapp.com/ gatsby build",
    "deploy:fb:dev": "npm run deploy:fb && firebase deploy",
    "deploy:ghpages": "cross-env GHPAGES=1 gatsby build --prefix-paths && node ./scripts/deploy-ghpages.js",
    "ci:deploy:fb": "npm run deploy:fb && firebase deploy --token \"$FIREBASE_TOKEN\"",
    "ci:lint": "eslint ./ --ext .js,.jsx,.ts,.tsx --quiet && prettier --check ./**/*.{js,jsx,ts,tsx,scss} && stylelint --quiet src/**/*.scss",
    "ci:test": "cross-env CYPRESS_SUPPORT=y start-server-and-test develop http://localhost:8000 cy:run:ci",
    "gatsby:build": "gatsby build",
    "gatsby:dev": "gatsby develop",
    "gatsby:dev:ssl": "gatsby develop --https",
    "hooks": "mkdir -p .git/hooks && npx lefthook install",
    "hooks:run": "npx lefthook run",
    "lint": "npm run lint:format && npm run lint:fix && npm run lint:style",
    "check:js": "eslint ./ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint ./ --ext .js,.jsx,.ts,.tsx --fix --quiet",
    "lint:format": "prettier --write ./**/*.{js,jsx,ts,tsx,scss} --loglevel error",
    "lint:style": "stylelint --fix --quiet src/**/*.scss",
    "test": "cypress open",
    "test:open": "cypress open",
    "test:dev": "cross-env CYPRESS_SUPPORT=y start-server-and-test dev http://localhost:8000 test:open",
    "test:run": "cross-env CYPRESS_SUPPORT=y start-server-and-test build http://localhost:8000 test:open",
    "test:record": "cypress run --record --key cd256083-c513-4506-89f7-4b74ca5cc79a",
    "test:perf": "ava tests/performance/lighthouse.js --verbose",
    "start": "gatsby develop",
    "serve": "gatsby serve"
  },
  "greenkeeper": {
    "ignore": [
      "ava",
      "cypresss",
      "core-js"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.10.0",
    "@material-ui/icons": "^4.9.1",
    "@reach/router": "~1.2.1",
    "@svgr/webpack": "^5.4.0",
    "add": "^2.0.6",
    "autoprefixer": "^9.8.0",
    "badgee": "~2.0.5",
    "breakpoint-sass": "2.7.1",
    "cssnano": "~4.1.10",
    "dotenv-flow": "~3.1.0",
    "gatsby": "^2.22.11",
    "gatsby-image": "^2.4.5",
    "gatsby-node-helpers": "~0.3.0",
    "gatsby-plugin-accessibilityjs": "~1.0.3",
    "gatsby-plugin-canonical-urls": "^2.3.2",
    "gatsby-plugin-catch-links": "^2.3.3",
    "gatsby-plugin-csp": "~1.1.3",
    "gatsby-plugin-extract-schema": "~0.0.5",
    "gatsby-plugin-fastclick": "~1.0.1",
    "gatsby-plugin-google-tagmanager": "^2.3.3",
    "gatsby-plugin-manifest": "^2.4.9",
    "gatsby-plugin-netlify": "^2.3.3",
    "gatsby-plugin-netlify-cache": "~1.2.0",
    "gatsby-plugin-nprogress": "^2.3.2",
    "gatsby-plugin-offline": "^3.2.7",
    "gatsby-plugin-page-creator": "^2.3.7",
    "gatsby-plugin-polyfill-io": "~1.1.0",
    "gatsby-plugin-postcss": "^2.3.2",
    "gatsby-plugin-prefetch-google-fonts": "~1.4.3",
    "gatsby-plugin-react-helmet": "^3.3.2",
    "gatsby-plugin-recaptcha": "~1.0.5",
    "gatsby-plugin-robots-txt": "~1.5.0",
    "gatsby-plugin-sass": "^2.3.2",
    "gatsby-plugin-sharp": "^2.6.9",
    "gatsby-plugin-sitemap": "^2.4.3",
    "gatsby-plugin-svgr": "~2.0.2",
    "gatsby-plugin-transition-link": "^1.18.0",
    "gatsby-source-drupal": "^3.5.11",
    "gatsby-source-filesystem": "^2.3.8",
    "gatsby-source-graphql": "^2.5.2",
    "gatsby-transformer-json": "^2.4.3",
    "gatsby-transformer-sharp": "^2.5.3",
    "gsap": "^3.2.6",
    "lodash": "~4.17.15",
    "moment": "^2.26.0",
    "node-sass": "^4.14.1",
    "postcss-extend": "~1.0.5",
    "postcss-import": "~12.0.1",
    "postcss-merge-rules": "~4.0.3",
    "postcss-nesting": "~7.0.0",
    "postcss-preset-env": "~6.7.0",
    "postcss-pxtorem": "~4.0.1",
    "postcss-scss": "^2.1.1",
    "prop-types": "~15.7.2",
    "react": "^16.13.1",
    "react-async-hook": "^3.6.1",
    "react-dom": "^16.13.1",
    "react-helmet": "6.0.0-beta",
    "react-hook-form": "^4.10.2",
    "react-id-swiper": "^3.0.0",
    "react-intersection-observer": "^8.26.2",
    "react-pose": "^4.0.10",
    "react-recaptcha": "~2.3.10",
    "sassy-normalize": "^8.0.5",
    "swiper": "^5.4.1",
    "yup": "^0.28.5"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.6.7",
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/plugin-proposal-optional-chaining": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/register": "^7.6.0",
    "@danbruegge/gatsby-plugin-stylelint": "^3.4.1",
    "@testing-library/cypress": "^4.2.0",
    "ava": "^2.4.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-gatsby": "^0.2.36",
    "chrome-launcher": "^0.13.3",
    "cross-env": "^5.2.1",
    "cypress": "^3.8.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "gatsby-cypress": "^0.2.24",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gh-pages": "^2.2.0",
    "lighthouse": "^5.6.0",
    "prettier": "^1.19.1",
    "resolve-url-loader": "^3.1.1",
    "start-server-and-test": "^1.11.0",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.3.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-order": "^3.1.1",
    "stylelint-scss": "^3.17.2",
    "workerize-loader": "^1.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/homer-yttc/gatsby-baseline"
  },
  "bugs": {
    "url": "https://github.com/homer-yttc/gatsby-baseline/issues"
  }
}
